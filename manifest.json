{
  "name": "hỗ trợ thi",
  "version": "1.0",
  "description": "Hỗ trợ thi",
  "background": {
    "scripts": [
      "background.js"
      
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://redmine.vti.com.vn/*time_entries/new"
      ],
      "js": [
        "js/jquery-3.5.1.min.js",
        "js/jquery-ui.js",
        "logtime.js",
        "sticknote.js"
      ],
      "css": [
        "css/logtime.css"
      ],
      "run_at": "document_idle",
      "all_frames": false
    },
    {
      "matches": [
        "*://vms.vti.com.vn/*"
      ],
      "js": [
        "js/jquery-3.5.1.min.js",
        "logOT.js"
      ],
      "css": [
        "css/logtime.css"
      ],
      "run_at": "document_idle",
      "all_frames": false
    },
    {
      "matches": [
        "*://learning2.ehou.edu.vn/mod/quiz/review.php?attempt=*"
      ],
      "js": [
        "js/jquery-3.5.1.min.js",
        "js/firebase-app.js",
        "js/firebase-firestore.js",
        "js/firebase-init.js",
        "js/firebase.js",
        "js/ehou.js"

      ],
      "css": [
        "css/logtime.css"
      ],
      "run_at": "document_idle",
      "all_frames": false
    },
    {
      "matches": [
        "*://learning2.ehou.edu.vn/mod/quiz/attempt.php?attempt=*"
      ],
      "js": [
        "js/jquery-3.5.1.min.js",
        "js/firebase-app.js",
        "js/firebase-firestore.js",
        "js/firebase-init.js",
        "js/firebase.js",
        "js/ehou-thi.js"

      ],
      "css": [
        "css/logtime.css"
      ],
      "run_at": "document_idle",
      "all_frames": false
    }
  ],
  "web_accessible_resources": ["ehou.html"],
  "browser_action": {
    "default_icon": "icon.png",
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "contextMenus",
    "tabs","<all_urls>",
    "http://*/*",
    "https://*/*"

  ],
  "manifest_version": 2
}