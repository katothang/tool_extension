{
  "name": "logtime tool",
  "version": "1.0",
  "description": "logtime redmine + log OT vms",
  "background": {
    "scripts": [
      "background.js"
      
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://redmine.vti.com.vn/*time_entries/new"
      ],
      "js": [
        "js/jquery-3.5.1.min.js",
        "js/jquery-ui.js",
        "logtime.js",
        "sticknote.js"
      ],
      "css": [
        "css/logtime.css"
      ],
      "run_at": "document_idle",
      "all_frames": false
    },
    {
      "matches": [
        "*://vms.vti.com.vn/*"
      ],
      "js": [
        "js/jquery-3.5.1.min.js",
        "logOT.js"
      ],
      "css": [
        "css/logtime.css"
      ],
      "run_at": "document_idle",
      "all_frames": false
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ],
  "manifest_version": 2
}